


local Library = loadstring(game:HttpGet("https://pastebin.com/raw/Q69rcHsK"))()
local colors = {
    SchemeColor = Color3.fromRGB(125, 102, 255),
    Background = Color3.fromRGB(0, 0, 0),
    Header = Color3.fromRGB(0, 0, 0),
    TextColor = Color3.fromRGB(0, 0, 255),
    ElementColor = Color3.fromRGB(20, 20, 20)
}
local Window = Library.CreateLib("üòék00pkid6660 op hubüòé ",colors) local themes = {
    "SchemeColor",
    "Background",
    "Header",
    "TextColor",
    "ElementColor"
}

--Color changer

local Tab = Window:NewTab("UI color changer")
local Section = Tab:NewSection("UI color changer")

for _, theme, color in ipairs(themes) do
    Section:NewColorPicker(theme, "Change your " .. theme, colors[theme], function(color)
        colors[theme] = color
    end)
end
local ToggleGui = Instance.new("ScreenGui")
local Toggle = Instance.new("TextButton")
local UICorner = Instance.new("UICorner")

--Properties:

local ToggleGui = Instance.new("ScreenGui")
local Toggle = Instance.new("TextButton")
local UICorner = Instance.new("UICorner")

ToggleGui.Name = "ToggleGui_HE"
ToggleGui.Parent = game.CoreGui

Toggle.Name = "Toggle"
Toggle.Parent = ToggleGui
Toggle.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
Toggle.Position = UDim2.new(0, 0, 0.427710831, 0)
Toggle.Size = UDim2.new(0, 85, 0, 39)
Toggle.Font = Enum.Font.SourceSans
Toggle.Text = "üòék00pkid6660 hubüòé"
Toggle.TextColor3 = Color3.fromRGB(255, 255, 255)
Toggle.TextSize = 18.000
Toggle.TextWrapped = true
Toggle.Active = true
Toggle.Draggable = true
Toggle.MouseButton1Click:connect(function()
    Library:ToggleUI()
end)
